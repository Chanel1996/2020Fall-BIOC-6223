{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://github.com/kee007ney/2020Fall-BIOC-6223/blob/master/Week01/Tianyi%20Wang%20schema.json",
	"type": "objects",
	"required": [
		"name",
		"length_in_bp",
		"Accession_number"
	],
	"properties": {
		"name": {
			"type": "string",
			"example": [
				"EGFR"
			]
		},
		"length_in_bp": {
			"type": "number",
			"example": [
				"1858"
			]
		},
		"Accession_number": {
			"type": "string",
			"example": [
				"AB025197"
			]
		}
	}
}
